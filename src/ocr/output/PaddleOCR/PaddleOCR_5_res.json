    "parsing_res_list": [
        {
            "block_label": "text",
            "block_content": "We project the refined queries into a shared relational space to compute the pairwise precedence score  $ S_{i,j} $:",
            "block_bbox": [
                137,
                173,
                1055,
                227
            ],
            "block_id": 0,
            "block_order": 1,
            "group_id": 0,
            "block_polygon_points": [
                [
                    137.0,

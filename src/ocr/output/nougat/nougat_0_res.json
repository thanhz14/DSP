                ]
            ]
        },
        {
            "block_label": "paragraph_title",
            "block_content": "1 Introduction",
            "block_bbox": [
                123,
                749,
                329,
                777
            ],
            "block_id": 9,
            "block_order": 9,
            "group_id": 9,
            "block_polygon_points": [
                [
                    123.0,
                    749.0
                ],
                [
                    329.0,
                    749.0
                ],
                [
                    329.0,
                    777.0
                ],
                [
                    123.0,
